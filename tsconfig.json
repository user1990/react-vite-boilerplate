{
  "compilerOptions": {
    "target": "ESNext",
    "importHelpers": true,
    "removeComments": true,
    "lib": ["DOM", "DOM.Iterable", "ESNext"],
    "allowJs": false,
    "skipLibCheck": true,
    "esModuleInterop": false,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "noUnusedLocals": false,
    "noImplicitAny": true,
    "jsx": "react-jsx",
    "baseUrl": ".",
    "types": ["vite-plugin-svgr/client", "vite/client"],
    "paths": {
      "common/*": ["src/common/*"],
      "modules/*": ["src/modules/*"],
      "pages/*": ["./src/pages/*"],
      "app/*": ["./src/app/*"],
      "config/*": ["./src/config/*"],
      "assets/*": ["./src/assets/*"],
      "utils/*": ["./src/utils/*"],
      "models/*": ["./src/models/*"],
      "types/*": ["./src/types/*"]
    }
  },
  "exclude": ["node_modules"],
  "include": ["./src"]
}
